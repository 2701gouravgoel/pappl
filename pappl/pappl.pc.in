prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: PAPPL
Description: Printer Application Framework/Library
Version: @PAPPL_VERSION@
URL: https://www.msweet.org/pappl
Requires: gnutls >= 3.0, libjpeg >= 9.0, libpng >= 1.6, libusb-1.0 >= 1.0, zlib >= 1.0
Libs: -L${libdir} -lpappl -lcups -lpam -lm
Cflags: -I${includedir}
